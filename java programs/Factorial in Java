
class Fact
{
    void  Factorial(int n)
    {
        int i, fact= 1;
        for(i =1  ; i<=n ; i++ )
        {
            fact = fact * i;  
        }
        System.out.println("Factorial for  " + n + " is " + fact);
    }
}

public class Main
{
	public static void main(String[] args) 
	{
	   
	   Fact f = new Fact();
	   f.Factorial(5);
	 
	}
	
}
